        <li>OnSkin</li>
      </layers>
      <defaultOutfitTags>
        <li>Outlander</li>
        <li>Offworlder</li>
      </defaultOutfitTags>
      <tags>
        <li>CA_Colonist</li>
      </tags>
    </apparel>
  </ThingDef>
